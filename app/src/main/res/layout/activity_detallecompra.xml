<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="57dp">

        <include layout="@layout/toolbar" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:padding="@dimen/paddingLayout">

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Detalle de Compra"
                android:textSize="24sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvNroTransac_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginTop="@dimen/margenCaja"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvFecha_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvConcierto_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvZona_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvCantidad_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvPrecio_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>

            <TextView
                android:id="@+id/tvMonto_DetalleCompra"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView"
                android:textSize="18sp"
                android:layout_marginBottom="@dimen/margenCaja"/>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>